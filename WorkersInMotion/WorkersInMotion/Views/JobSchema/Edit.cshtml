        function savePageDetails() {
            if ($("#Page_Name").val() != "" && $("#Page_Order").val() != "") {
                $.ajax({
                    type: "POST",
                    url: "/JobSchema/Page",
                    data: {
                        joblogicalID: $("#joblogicalid").val(),
                        pageName: $("#Page_Name").val(),
                        pageOrder: $("#Page_Order").val(),
