@model WorkersInMotion.Model.ViewModel.JobSchemaEditView

@{
    //Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
    <style type="text/css">
        /*.leftbar-heading {
            font-family: Arial,Verdana, Helvetica, sans-serif;
            font-size: 13px;
            font-style: normal;
            line-height: normal;
            font-weight: normal;
            font-variant: normal;
            text-transform: none;
        }*/
    </style>
</head>
<body>
    <!-- BEGIN PAGE HEADER-->
    <div class="row">
        <div class="col-md-12">
            <!-- BEGIN PAGE TITLE & BREADCRUMB-->
            <h3 class="page-title">
                Edit Job Form
            </h3>
            <ul class="page-breadcrumb breadcrumb">
                <li>
                    <i class="icon-home"></i>
                    <a href="@Url.Action("DashBoard","User")">Home</a>
                    <i class="icon-angle-right"></i>
                </li>
                <li><a href="@Url.Action("Index","JobSchema")">Job Form</a></li>
            </ul>
            <!-- END PAGE TITLE & BREADCRUMB-->
        </div>
    </div>
    <!-- END PAGE HEADER-->
    <!-- BEGIN DASHBOARD STATS -->
    <div class="row">
    </div>
    <!-- END DASHBOARD STATS -->


    <div class="clearfix"></div>
    <div class="row">
        <div class="col-md-12 col-sm-12">
            <!-- BEGIN PORTLET-->
            <div class="portlet box grey">
                <div class="portlet-title">
                    <div class="caption"><i class="icon-cog"></i>This page will allow you to edit a Job Form.</div>
                </div>
                <div class="portlet-body form">
                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()

                        <div class="form-horizontal">
                            <div>
                                <div class="leftbar-heading-border" style="width: 40%; padding: 10px; float: left">
                                    <input type="hidden" id="joblogicalid" value="@ViewBag.JobLogicalID" />
                                    <table class="table table-striped table-bordered table-hover">
                                        <tr>
                                            <td>
                                                Job Class Name:

                                                @ViewData["JobName"].ToString()
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                Job Form ID : @ViewData["JobClass"].ToString()
                                            </td>
                                        </tr>
                                        @*<tr>
                                                <td>
                                                    Worker Group :

                                                    @ViewData["GroupName"].ToString()
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    Estimated Duration (In Hours) :

                                                    @ViewData["EstimatedDuration"].ToString()
                                                </td>
                                            </tr>*@
                                    </table>
                                </div>
                                <div id="pagedetails" class="leftbar-heading-border" style="display:none;float:left;width:50%;padding:20px;">
                                    <table>
                                        <tr>
                                            <td>
                                                @Html.Label("Page Name", new { @Style = "padding-right:5px" })
                                            </td>
                                            <td>
                                                @Html.Editor("Page_Name")
                                                @Html.ValidationMessage("Page_Name")
                                            </td>
                                        </tr>
                                        <tr>
                                            <td colspan="2">
                                                <br />
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                @Html.Label("Page Order", new { @Style = "padding-right:5px" })
                                            </td>
                                            <td>
                                                @Html.Editor("Page_Order")
                                                @Html.ValidationMessage("Page_Order")
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                @Html.Label("Can Repeat Page", new { @Style = "padding-right:5px" })
                                            </td>
                                            <td>
                                                @Html.CheckBox("Can_Repeat")
                                                @Html.ValidationMessage("Can_Repeat")
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                            </div>
                            <div class="form-actions right" align="right">
                                <button type="submit" id="btnaddPage" style="display:block" class="btn blue" onclick="return displaypageentry();"><i class="icon-ok"></i> Add Page</button>
                                <button type="submit" id="btnsavePage" style="display:none" class="btn blue" onclick="return savePageDetails();"><i class="icon-ok"></i> Save Page</button>
                                <button type="submit" id="btnaddItem" style="display:none" class="btn blue"><i class="icon-ok"></i> Add Item </button><br />
                                <button type="submit" id="btndeletePage" style="display:none" class="btn blue" onclick="return deletePageDetails();"><i class="icon-ok"></i> Delete Page</button><br />
                                <div>
                                    <a href="@Url.Action("Editjob", "JobSchema" )" id="btnFinish" class="btn blue">
                                        <i class="icon-ok"></i>   Finish
                                    </a>
                                </div>
                            </div>
                            <div id="pageListDetails" style="display:block">
                                @foreach (var item in Model.JobPageSchemaModel)
                                {
                                    @Html.RadioButton("rdoPageList", item.PageLogicalID, new { @id = "rdo_btn_" + item.PageLogicalID })
                                    @Html.Label(item.PageSchemaName, new { @for = "rdo_btn_" + item.PageLogicalID })<br />
                                }
                            </div>
                            <div id="SelectButtonGroup" style="float:left;width:60%"></div>
                            <div id="SelectBoxDiv" style="float:left">
                                @foreach (var item in Model.JobPageSchemaModel)
                                {
                                    <div id="Div@(item.PageLogicalID)">

                                        <fieldset style="padding-left:20px;width:98%;padding-top:20px;padding-bottom:20px;border:solid 1px #CBCDCF;">
                                            <legend style="padding-left:10px;padding-right:10px;border:1px solid #CBCDCF; font-size:13px;font-weight:bold;height:20px;vertical-align:middle;background-color:#F5F5F6;">@(item.PageSchemaName)</legend>
                                            <div id="Div_@(item.PageLogicalID)">
                                                @foreach (var item1 in Model.JobPageItemModel.Where(x => x.PageLogicalID == item.PageLogicalID))
                                                {
                                                    if (item1.ItemControlType == "text" || item1.ItemControlType == "decimal" || item1.ItemControlType == "image" || item1.ItemControlType == "audio" || item1.ItemControlType == "number" || item1.ItemControlType == "phone" || item1.ItemControlType == "date" || item1.ItemControlType == "email")
                                                    {
                                                        <div id="Div@(item1.ItemLogicalID)" class="delete">
                                                            <label class="delete">@item1.ItemName</label><label class="delete" style="padding-left:50px;padding-bottom:10px;">:</label>
                                                            <input type="text" class="delete" disabled="disabled" style="background-color:#FFFFFD;width:120px;" />&nbsp;
                                                            <input id="btn" type="button" class="btnsmall" onclick="return deletepageItem('@(item1.ItemLogicalID)');" value="Remove" /><br /><br />
                                                        </div>
                                                    }
                                                    if (item1.ItemControlType == "check")
                                                    {
                                                        if (item1.ItemValue == "1")
                                                        {
                                                            <div id="Div@(item1.ItemLogicalID)" class="delete">
                                                                <label class="delete">@item1.ItemName</label>
                                                                <label class="delete" style="padding-left:50px;padding-bottom:10px;">:</label>
                                                                <input class="delete" type="checkbox" disabled="disabled" checked="checked" style="background-color:#FFFFFD;" />&nbsp;
                                                                <input id="btn" type="button" class="btnsmall" onclick="return deletepageItem('@(item1.ItemLogicalID)');" value="Remove" /><br /><br />
                                                            </div>
                                                        }
                                                        else
                                                        {
                                                            <div id="Div@(item1.ItemLogicalID)" class="delete">
                                                                <label class="delete">@item1.ItemName</label>
                                                                <label class="delete" style="padding-left:50px;padding-bottom:10px;">:</label>
                                                                <input class="delete" type="checkbox" disabled="disabled" style="background-color:#FFFFFD;" />&nbsp;
                                                                <input id="btn" type="button" class="btnsmall" onclick="return deletepageItem('@(item1.ItemLogicalID)');" value="Remove" /><br /><br />
                                                            </div>
                                                        }
                                                    }
                                                    if (item1.ItemControlType == "multitext")
                                                    {
                                                        <div id="Div@(item1.ItemLogicalID)" class="delete">
                                                            <table>
                                                                <tr>
                                                                    <td>
                                                                        <label class="delete">@item1.ItemName</label>
                                                                    </td>
                                                                    <td>
                                                                        <label class="delete" style="padding-left:50px;padding-bottom:10px;">:</label>
                                                                    </td>
                                                                    <td>
                                                                        @*@Html.TextArea(item1.ItemName)*@
                                                                        <textarea cols="20" rows="2" style="background-color:#FFFFFD;"></textarea>
                                                                    </td>
                                                                    <td>
                                                                        <input id="btn" type="button" class="btnsmall" onclick="return deletepageItem('@(item1.ItemLogicalID)');" value="Remove" />
                                                                    </td>

                                                                </tr>
                                                            </table>
                                                        </div>
                                                    }
                                                    if (item1.ItemControlType == "label")
                                                    {
                                                        <div id="Div@(item1.ItemLogicalID)" class="delete">
                                                            <label class="delete">@item1.ItemName</label>
                                                            <label class="delete" style="padding-left:50px;padding-bottom:10px;">:</label>
                                                            <label class="delete">&nbsp;&nbsp;@item1.ItemValue</label>&nbsp;
                                                            <input id="btn" type="button" class="btnsmall" onclick="return deletepageItem('@(item1.ItemLogicalID)');" value="Remove" /><br /><br />
                                                        </div>
                                                    }
                                                    if (item1.ItemControlType == "dropdown")
                                                    {
                                                        <div id="Div@(item1.ItemLogicalID)" class="delete">
                                                            <table>
                                                                <tr>
                                                                    <td>
                                                                        <label class="delete">@item1.ItemName</label>
                                                                    </td>
                                                                    <td>
                                                                        <label class="delete" style="padding-left:50px;padding-bottom:10px;">:</label>
                                                                    </td>
                                                                    <td class="dropdownbtn" data-value="@item1.ItemValueType$@item1.ItemName$@item1.ItemLogicalID"></td>
                                                                    <td>&nbsp;<input id="btn" type="button" class="btnsmall" onclick="return deletepageItem('@(item1.ItemLogicalID)');" value="Remove" /></td>
                                                                </tr>
                                                                <tr>
                                                                    <br />
                                                                    <br />
                                                                </tr>
                                                            </table>
                                                        </div>
                                                    }
                                                    if (item1.ItemControlType == "radio")
                                                    {
                                                        <div id="Div@(item1.ItemLogicalID)" class="delete">
                                                            <table>
                                                                <tr>
                                                                    <td>
                                                                        <label class="delete">@item1.ItemName</label>&nbsp;
                                                                    </td>
                                                                    <td><label class="delete" style="padding-left:50px;padding-bottom:10px;">:</label></td>
                                                                    <td class="radiobtn" data-value="@item1.ItemValueType$@item1.ItemName$@item1.ItemLogicalID"></td>
                                                                    <td>&nbsp;<input id="btn" type="button" class="btnsmall" onclick="return deletepageItem('@(item1.ItemLogicalID)');" value="Remove" /></td>
                                                                <tr>
                                                                    <br />
                                                                    <br />
                                                            </table>
                                                        </div>
                                                    }
                                                }
                                            </div>
                                        </fieldset>

                                    </div>
                                }
                            </div>
                        </div>
                    }
                </div>

            </div>
            <!-- END PORTLET-->
        </div>
        <div class="clearfix"></div>

    </div>
    <!-- END PAGE -->
