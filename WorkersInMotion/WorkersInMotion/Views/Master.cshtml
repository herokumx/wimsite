<script>
    $(function () {
        findurl();
    });

    jQuery(document).ready(function () {
        App.init(); // initlayout and core plugins
        Index.init();
        Index.initJQVMAP(); // init index page's custom scripts
        Index.initCalendar(); // init index page's custom scripts
        // Index.initCharts(); // init index page's custom scripts
        Index.initChat();
        // Index.initPieCharts();
        Index.initMiniCharts();
        Index.initDashboardDaterange();
        //Index.initIntro();
        Tasks.initDashboardWidget();
    });
    function findurl() {
        var url = "";
        var urlarray = location.pathname.split('/');
        if (urlarray.length > 0)
            url = urlarray[1];
        if (url == "Territory") {
            url = "Region";
        }
        //else {
        //    url = urlarray[urlarray.length - 1];
        //}
        // alert(url);
        // $("li[class='active']").removeClass("active");
        $("#newdiv a[href*='" + url + "']").parent().attr("class", "active open");
        $("#newdiv a[href*='" + url + "']").parent().parent().parent().attr("class", "active open");
        $("#newdiv a[href*='" + url + "']").parent().parent().parent().find("span.arrow").addClass("open");
        $("#newdiv a[href*='" + url + "']").parent().parent().parent().find("ul.sub-menu").css({ "display": "block" });


    }

</script>