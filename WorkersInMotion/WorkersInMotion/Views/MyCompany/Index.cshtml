                                            </li>
                                            @if (Session["OrganizationName"] != null && !string.IsNullOrEmpty(Session["OrganizationName"].ToString()))
                                            {
                                            <li>
                                                <a href="#">@Session["OrganizationName"].ToString()</a>
                                            </li>
                                            }
                                        </ul>*@
