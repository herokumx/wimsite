                                                                    @Html.LabelFor(model => model.OrganizationEditView.State, new { @class = " control-label" })
                                                                    @Html.TextBoxFor(model => model.OrganizationEditView.State, new { @class = "form-control" })
                                                                    @Html.ValidationMessageFor(model => model.OrganizationEditView.State)
