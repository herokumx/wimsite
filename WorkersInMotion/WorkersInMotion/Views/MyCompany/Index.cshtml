                                                                    @Html.TextBoxFor(model => model.OrganizationEditView.Country, new { @class = "form-control" })
                                                                    @Html.ValidationMessageFor(model => model.OrganizationEditView.Country)
                                                                </div>
                                                            </div>
                                                            <div class="col-md-6">
                                                                <div class="form-group">
                                                                    @Html.LabelFor(model => model.OrganizationEditView.ZipCode, new { @class = "control-label" })
                                                                    @Html.TextBoxFor(model => model.OrganizationEditView.ZipCode, new { @class = "form-control" })
                                                                    @Html.ValidationMessageFor(model => model.OrganizationEditView.ZipCode)
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="col-md-6">
                                                                <div class="form-group">
                                                                    @Html.LabelFor(model => model.OrganizationEditView.EmailID, new { @class = "control-label" })
                                                                    @Html.TextBoxFor(model => model.OrganizationEditView.EmailID, new { @class = "form-control" })
                                                                    @Html.ValidationMessageFor(model => model.OrganizationEditView.EmailID)
