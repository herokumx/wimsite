                                                                            if (item.IsActive)
                                                                            {
                                                                                @Html.ActionLink("UnSubscribe", "UnSubscribe", "User", new { id = item.SubscriptionGUID }, new { @class = "label label-sm label-default", @style = "border-radius:0px;margin-right:4px", @onclick = "return Unsubscribeconfirm();" })
                                                                            }
                                                                            else
                                                                            {
                                                                                @Html.ActionLink("Subscribe", "Subscribe", "User", new { id = item.SubscriptionGUID }, new { @class = "label label-sm label-default", @style = "border-radius:0px;margin-right:4px", @onclick = "return subscribeconfirm();" })
                                                                            }

                                                                        }
                                                                    }
                                                                    @if (item.UserType != "ENT_A")
                                                                    {
                                                                        @Html.ActionLink("Delete", "Delete", "User", new { id = item.Id }, new { @id = "delete", @class = "label label-sm label-warning", @style = "border-radius:0px;margin-right:4px", @onclick = "return deleteconfirm();" })
                                                                    }
                                                                </td>
                                                            }


                                                        </tr>
                                                    }
                                                }
                                                else
                                                {
                                                    <tr>
                                                        <td colspan="6" align="center" style="color:red">
                                                            No Data Found
                                                        </td>
                                                    </tr>
                                                }
                                            </tbody>
                                        </table>


                                    </div><div class="row"></div>
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
