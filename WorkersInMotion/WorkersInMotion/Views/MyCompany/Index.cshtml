                                                        <th class="sorting" role="columnheader" tabindex="0" aria-controls="sample_editable_3" rowspan="1" colspan="1" style="width: 240px;" aria-label="
                                        Description
                                    : activate to sort column ascending">
                                                            Description
                                                        </th>
                                                        <th class="sorting-disabled" role="columnheader" tabindex="0" aria-controls="sample_editable_3" rowspan="1" colspan="1" style="width: 101px;" aria-label="
                                         Actions
                                    : activate to sort column ascending">
                                                            Actions
                                                        </th>
                                                    }
                                                    else
                                                    {
                                                        <th class="sorting-disabled" role="columnheader" tabindex="0" aria-controls="sample_editable_3" rowspan="1" colspan="1" style="width: 240px;" aria-label="
                                        Description
                                    : activate to sort column ascending">
                                                            Description
                                                        </th>
                                                    }
                                                </tr>
                                            </thead>

                                            <tbody role="alert" aria-live="polite" aria-relevant="all">
                                                @if (Model.TerritoryViewModel != null && Model.TerritoryViewModel.Territory.Count() > 0)
                                                {
                                                    foreach (var item in Model.TerritoryViewModel.Territory)
                                                    {
                                                        <tr class="odd gradeX">

                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.Name)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.Description)
                                                            </td>
                                                            @if (Session["UserType"] != null && !string.IsNullOrEmpty(Session["UserType"].ToString()) && Session["UserType"].ToString() == "ENT_A")
                                                            {
                                                                <td>
                                                                    @*@Html.ActionLink("Service Points", "Index", "ServicePoint", new { id = item.TerritoryGUID, regionguid = item.RegionGUID }, new { @class = "label label-sm label-default", @style = "border-radius:0px;" })*@


                                                                    @Html.ActionLink("Edit", "Edit", "Territory", new { id = item.TerritoryGUID }, new { @class = "label label-sm label-info", @style = "border-radius:0px;margin-right:3px" })

                                                                    @Html.ActionLink("Delete", "Delete", "Territory", new { id = item.RegionGUID, id1 = item.TerritoryGUID }, new { @class = "label label-sm label-warning", @style = "border-radius:0px;", @onclick = "return deleteconfirm();" })

