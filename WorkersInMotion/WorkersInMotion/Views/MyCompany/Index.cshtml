                                                    @if (Session["UserType"] != null && !string.IsNullOrEmpty(Session["UserType"].ToString()) && Session["UserType"].ToString() == "ENT_A")
                                                    {
                                                        <div class="form-actions right">
                                                            @*<a href="@Url.Action(" index", "MyCompany" , new { id="Details" })" class="btn default">Cancel</a>*@
                                                            <button type="submit" class="btn blue"><i class="icon-ok"></i> Update</button>
                                                        </div>
                                                    }
                                                </div>
                                            }

                                        </div>
                                    </div>
                                    <!-- END PORTLET-->
                                </div>
                                <div class="clearfix"></div>

                            </div>
                        }
                    </div>
                    <div class="@(TempData["TabName"].ToString() == "Regions" ? "tab-pane active" : "tab-pane")" id="tab_5_2">
                        @if (Model.RegionViewModel != null && Model.RegionViewModel.Region != null)
                        {
                            <div class="portlet box grey">
                                <div class="portlet-title">
                                    @if (Session["UserType"] != null && !string.IsNullOrEmpty(Session["UserType"].ToString()) && Session["UserType"].ToString() == "ENT_A")
                                    {
                                        <div class="caption"><i class="icon-cog"></i>This page will allow you to add a region.</div>
                                    }
                                    else
                                    {
                                        <div class="caption"><i class="icon-cog"></i> This page displays the regions of the organization.</div>
                                    }
                                </div>
                                <div class="portlet-body">
                                    <div class="table-toolbar">
                                        <div class="btn-group">
                                            @if (Session["UserType"] != null && !string.IsNullOrEmpty(Session["UserType"].ToString()) && Session["UserType"].ToString() == "ENT_A")
                                            {
                                                <a href="@Url.Action("Create", "Region" )" class="btn green">
                                                    Add New Region <i class='icon-plus'></i>
                                                </a>
                                            }
                                        </div>
                                    </div>
                                    <div id="sample_editable_2_wrapper" class="dataTables_wrapper" role="grid">
                                        <table class="table table-striped table-hover table-bordered dataTable" id="sample_editable_2" aria-describedby="sample_editable_2_info" style="width: 1065px;">
                                            <thead>
                                                <tr role="row">
                                                    <th class="sorting" role="columnheader" tabindex="0" aria-controls="sample_editable_2" rowspan="1" colspan="1" style="width: 233px;" aria-label="
                                         Name
                                    : activate to sort column ascending">
                                                        Name
                                                    </th>
                                                    <th class="sorting" role="columnheader" tabindex="0" aria-controls="sample_editable_2" rowspan="1" colspan="1" style="width: 240px;" aria-label="
                                        Description
                                    : activate to sort column ascending">
                                                        Description
                                                    </th>
                                                    <th class="sorting-disabled" role="columnheader" tabindex="0" aria-controls="sample_editable_2" rowspan="1" colspan="1" style="width: 101px;" aria-label="
                                         Actions
                                    : activate to sort column ascending">
                                                        Actions
                                                    </th>
                                                </tr>
                                            </thead>

                                            <tbody role="alert" aria-live="polite" aria-relevant="all">
                                                @if (Model.RegionViewModel != null && Model.RegionViewModel.Region.Count() > 0)
                                                {
                                                    foreach (var item in Model.RegionViewModel.Region)
                                                    {
                                                        <tr class="odd gradeX">
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.Name)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.Description)
                                                            </td>
                                                            <td>
                                                                @if (Session["UserType"] != null && !string.IsNullOrEmpty(Session["UserType"].ToString()) && Session["UserType"].ToString() == "ENT_A")
                                                                {

                                                                    @Html.ActionLink("Edit", "Edit", "Region", new { id = item.RegionGUID }, new { @class = "label label-sm label-info", @style = "border-radius:0px;" })
                                                                }
                                                                @Html.ActionLink("Markets", "Index", "MyCompany", new { regionguid = item.RegionGUID }, new { @class = "label label-sm label-default", @style = "border-radius:0px;" })
                                                                @if (Session["UserType"] != null && !string.IsNullOrEmpty(Session["UserType"].ToString()) && Session["UserType"].ToString() == "ENT_A")
                                                                {
                                                                    @Html.ActionLink("Delete", "Delete", "Region", new { id = item.RegionGUID }, new { @class = "label label-sm label-warning", @style = "border-radius:0px;", @onclick = "return deleteconfirm();" })
                                                                }
                                                            </td>

                                                        </tr>
                                                    }
                                                }
                                                else
                                                {
                                                    <tr>
                                                        <td colspan="3" align="center" style="color:red">
                                                            No Data Found
                                                        </td>
                                                    </tr>
                                                }
                                            </tbody>
                                        </table>


                                    </div><div class="row"></div>
                                </div>
                            </div>
                        }
                    </div>
                    <div class="@(TempData["TabName"].ToString() == "Markets" ? "tab-pane active" : "tab-pane")" id="tab_5_3">
                        @if (Model.TerritoryViewModel != null && Model.TerritoryViewModel.Territory != null)
